.dialogBox-error .dialogBox-close:hover{background:#333}
.dialogBox-success .dialogBox-hd{background:#390}
.dialogBox-success .dialogBox-close{background:#444}
.dialogBox-success .dialogBox-close:hover{background:#333}
.dialogBox-simple .dialogBox-bd{text-align:center;min-height:0;padding:30px 0}


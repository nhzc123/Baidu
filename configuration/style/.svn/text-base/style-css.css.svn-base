/* CSS Reset v2.0.1
=========================================================== */
html{-overflow-y:scroll}
body{color:#000;background:#fff;font-size:12px;line-height:1.666;-webkit-text-size-adjust:none}
body,textarea{font-family:arial}
body,h1,h2,h3,h4,h5,h6,ul,ol,li,form,p,dl,dt,dd,blockquote,legend,table,th,td,fieldset,menu,pre{margin:0;padding:0}
table,th,td,img,fieldset{border:0}
address,cite,em,code,var,dfn,ins,i,th,caption{font-style:normal;text-decoration:none}
h1,h2,h3,h4,h5,h6,th{font-size:100%;font-weight:normal}
input,select,button,textarea,table{margin:0;font-family:inherit;font-size:100%}
abbr,acronym{border:0;font-variant:normal}
q:before,q:after{content:""}
ul,ol{list-style:none}
table{border-collapse:collapse;border-spacing:0}
th,caption{text-align:left}

/* layout&public
=========================================================== */
a{text-decoration:none;color:#69879B}
a:hover{text-decoration:underline}
body{font-family:"Microsoft Yahei",arial;background:#BDC0BA;color:#585858}
button{visibility:visible}
.header{background:#434844;background:rgba(55,60,56,.9);color:#fff;height:50px;padding:0 20px;position:relative;z-index:99}
.footer{position:fixed;left:10px;bottom:10px}
.body{padding:10px;overflow:hidden;zoom:1}
input[type="checkbox"],
input[type="radio"]{width:16px;height:16px;vertical-align:middle}

.att-list-item,
.btn,
.ico,
.fn-ib{display:inline-block;vertical-align:middle}

.plus,
.plus-item,
.ico-arr-bdr,
.ico-arr-main,
.ico-maxContent,
.mod{
	-webkit-transition-property:-webkit-transform,width,padding,margin,left,right,top,bottom,background,color,box-shadow,border,opacity;
	-moz-transition-property:-moz-transform,padding,margin,left,right,top,bottom,background,color,box-shadow,border,opacity;
	-o-transition-property:-o-transform,padding,margin,left,right,top,bottom,background,color,box-shadow,border,opacity;
	transition-property:transform,padding,margin,left,right,top,bottom,background,color,box-shadow,border,opacity;
	-webkit-transition-duration:0.5s;
	-moz-transition-duration:0.5s;
	-o-transition-duration:0.5s;
	transition-duration:0.5s;
}
.ico-maxContent,
.content{
	-webkit-transition-property:-webkit-transform,padding,margin,background,color,box-shadow,border,opacity;
	-moz-transition-property:-moz-transform,padding,margin,background,color,box-shadow,border,opacity;
	-o-transition-property:-o-transform,padding,margin,background,color,box-shadow,border,opacity;
	transition-property:transform,padding,margin,background,color,box-shadow,border,opacity;
}

.ico-maxContent{
	-webkit-transition-delay:.5s;
	-moz-transition-delay:.5s;
	-o-transition-delay:.5s;
	transition-delay:.5s;
}

.userInfo{-webkit-transition:none;-moz-transition:none;-o-transition:none;transition:none}

.ico{overflow:hidden}

.ico-att{width:26px;height:22px;background:url(../img/att.gif) no-repeat}
.ico-att-bubble{position:absolute;left:0;top:0;background:#69879B;padding:0 3px;height:13px;line-height:13px;text-align:center;border-radius:13px;color:#fff;font-weight:normal}

.ico-uarr,
.ico-darr{color:#c30;font-weight:bold;font-family:simsun}
.ico-darr{color:#3c0}

.ico-add{width:14px;height:14px;background:url(../img/add.gif)}

.ico-arr{width:10px;height:20px;line-height:20px;overflow:hidden;font-size:20px;font-family:simsun;text-indent:-11px;position:relative}
.ico-arr-bdr,
.ico-arr-main{position:relative}
.ico-arr-main{z-index:2}
.ico-arr-bdr{left:-20px;z-index:1}
.ico-arr-l{text-indent:0}
.ico-arr-l .ico-arr-bdr{left:-22px}
.ico-arr-b{text-indent:0;width:20px;height:10px}
.ico-arr-b .ico-arr-main{top:-10px}
.ico-arr-b .ico-arr-bdr{top:-9px;left:-21px}

.ico-coolArr{background:transparent;border-color:transparent;overflow:hidden;border-style:dashed;border-width:4px;width:0;height:0;line-height:normal!important}
.ico-coolArr-down{border-top-color:#fff;border-top-style:solid;border-bottom:0}
.ico-coolArr-up{border-bottom-color:#fff;border-bottom-style:solid;border-bottom-width:5px;border-top:0}
.ico-coolArr-right{border-left-color:#fff;border-left-style:solid;border-right:0}
.ico-coolArr-left{border-right-color:#fff;border-right-style:solid;border-right-width:5px;border-left:0}

.ico-num{position:absolute;left:100%;top:-4px;background:#c00;color:#fff!important;line-height:1;font-family:arial;font-size:9px;padding:2px 4px;border-radius:6px;font-size:10px;box-shadow:0 0 3px rgba(0,0,0,.5);font-weight:normal}

.ico-new{background:url(../img/new.gif);width:21px;height:10px}
.ico-done{background:url(../img/done.gif);width:24px;height:10px}
.ico-demo{background:url(../img/demo.gif);width:25px;height:10px}
.ico-beta{background:url(../img/beta.gif);width:24px;height:10px}
.ico-mail{background:url(../img/mail.gif);width:16px;height:11px}
.ico-mail-active{background-position:0 -11px}
.ico-mail-sending{height:15px;background-position:0 -11px}

.ico-maxContent{cursor:pointer;opacity:0;width:16px;height:16px;margin:-3px 0 0 -3px;background-color:#373C38;background-color:rgba(55,60,56,.9);background-position:50%;background-image:url(data:image/gif;base64,R0lGODlhEAAQAIAAAP///////yH5BAEHAAEALAAAAAAQABAAAAIbjI+py+1vgJyg0Wnxu7tqJkUhWIkl9HXQykIFADs=);background-repeat:no-repeat}
.ico-maxContent{position:absolute;left:0;top:0;z-index:9}
.ico-maxContent:hover{opacity:.8;padding:4px;margin:-7px 0 0 -7px}
.ico-maxContent-max{opacity:.4;left:10px!important;top:63px!important;background-position:50%;background-image:url(data:image/gif;base64,R0lGODlhEAAQAIAAAP///////yH5BAEHAAEALAAAAAAQABAAAAIdjI+py60AHIJRGnpgw0Fv7X2cQ1VLOT6oabXuKxUAOw==)}

.txt-impt{color:#c30!important}
.txt-aside{color:#999!important}


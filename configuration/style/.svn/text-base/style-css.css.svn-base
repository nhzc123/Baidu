.dialogBox{z-index:999;padding:5px;width:495px;position:fixed;background:#fff;border:1px solid #e0e0e0;box-shadow:0 0 15px rgba(0,0,0,.3)}
.dialogBox-hd{padding:4px 10px;margin:-6px -6px 0;background:#434844;color:#fff}
.dialogBox-hd h3{font-size:14px;font-weight:bold}
.dialogBox-bd{padding:20px;min-height:100px;max-height:500px;overflow:auto;position:relative;zoom:1}
.dialogBox-close{position:absolute;z-index:1;right:-8px;top:-8px;width:20px;height:20px;text-align:center;line-height:20px;color:#fff;background:#c30;font-weight:bold;border-radius:100%;box-shadow:0 0 5px rgba(0,0,0,.5)}
.dialogBox-close:hover{color:#fff;background:#f00;text-decoration:none}
.dialogBox-ft{margin:0 -6px -6px;padding:6px 4px;background:#f8f8f8;text-align:right}
.dialogBox-btn{margin-left:3px;line-height:normal;padding:3px 8px}
.dialogBox-error .dialogBox-bd,
.dialogBox-success .dialogBox-bd{min-height:50px;padding:10px;font-size:14px}
.dialogBox-error .dialogBox-hd{background:#c00}
